ul.captionList{
    width: 100%;
    clear: both;
    display: inline-block;
    margin: 0;
    padding: 0;
}

ul.captionList li{
    float: left;
    width: 30%;
}

ul.captionList li figure{
    margin: 0;
    padding: 0;
    text-align: center;
}

ul.captionList li figure img{
    width: 200px;
    height: 200px;
    border-radius: 100px;
    -webkit-transition: width 2s; 
    -moz-transition: 2s; 
    transition: 2s;
}

ul.captionList li figure img:hover{
    -webkit-filter: grayscale(100%); 
     -moz-filter: grayscale(100%); 
    filter: grayscale(100%);
}



ul.captionList li:nth-child(3){
    float: right;
}

ul.captionList li:nth-child(2){
    margin-left: 5%;
}


